   "source": [
    "import numpy as np\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1/(1 + np.exp(-x))\n",
    "\n",
    "i = [1.0, 0.5]\n",
    "w = [[0.9, 0.3], [0.2, 0.8]]\n",
    "\n",
    "output = sigmoid(np.dot(w, i))\n",
    "\n",
    "print(output)"
   ]
